        <div class="mk-row">
                                                                    <div class="mk-col-xs-12 mk-col-m-4">
                                                                                                                                        <a data-id="" class="mk_nav-menu__item mk_nav-menu__submenu-control mk_nav-menu--vintage" href="/fr/categorie/art-spectacles-salons-22340?fh_location=//meyclub/fr_FR/@PERSO/categories&lt;{meyclub_40}/categories&lt;{meyclub_40_22340}&amp;region_utilisateur=@REGION&amp;id_groupe=@IDGROUPE&amp;id_client=@IDCLIENT&amp;id_client_int=@IDCLIENTINT&amp;datetime=@DATETIME&amp;fh_view_size=20&amp;fh_start_index=0&amp;fh_suppress=redirect&amp;fh_maxdisplaynrvalues_avantage=-1">
